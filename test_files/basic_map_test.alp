module basic_map_test

export get/2, add/3, test_map/1, test_tuple_key_map/1

type map_result 'x = Ok 'x | NotFound

let test_map () = #{:one => 1, :two => 2}

let test_tuple_key_map () =
  #{(:one, 1) => "a", (:two, 2) => "b"}

let get k m = match m with
    #{k => v} -> Ok v
  | _ -> NotFound

let add k v m = #{k => v | m}